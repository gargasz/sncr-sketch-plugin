{
	"name": "SNCR",
	"description": "Synchronoss plugins to prepare Sketch project for wireframe production",
	"author": "Jason Burns",
	"homepage": "http://github.com/example/sketchplugins",
	"version": 0.3,
	"identifier": "com.example.sketch.sncr",
	"updateURL": "https://github.com/downloads/example/sketchplugins/sketchplugins.json",
	"compatibleVersion": 3,
	"bundleVersion": 1,
	"commands": [
		{
			"name": "1. Layout Artboards",
			"identifier": "layout",
			"script": "layout.js"
		},
		{
			"name": "2. Create Bounds",
			"identifier": "bounds",
			"script": "bounds.js"
		},
		{
			"name": "3. Generate Titles",
			"identifier": "generate",
			"script": "generate.js"
		},
		{
			"name": "4. Export Wireframes",
			"identifier": "export",
			"script": "export.js"
		},
		{
			"name": "Select Symbol Instances on Page",
			"identifier": "select-symbol-instances",
			"script": "select-symbol-instances.js"
		},
		{
			"name": "Match Instance Name to Symbol Name",
			"identifier": "match-instance-name",
			"script": "match-instance-name.js"
		}
	],
	"menu": {
		"items": [
			"layout",
			"bounds",
			"generate",
			"export",
			"select-symbol-instances",
			"match-instance-name"
		]
	}
}