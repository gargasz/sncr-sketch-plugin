{
	"name": "SNCR Plugins",
	"description": "Sketch plugins for Synchronoss",
	"author": "Jason Burns",
	"homepage": "https://github.com/sonburn/sncr-sketch-plugin",
	"version": 0.4,
	"identifier": "com.sncr.sketch",
	"compatibleVersion": 3,
	"bundleVersion": 1,
	"commands": [
		{
			"name": "Layout Artboards for Wireframe Export",
			"identifier": "artboard-wireframe-layout",
			"script": "artboard-wireframe-layout.js"
		},
		{
			"name": "Create Wireframes Bounds Around Artboards",
			"identifier": "create-artboard-bounds",
			"script": "create-artboard-bounds.js"
		},
		{
			"name": "Generate Titles Above Artboards",
			"identifier": "generate-artboard-titles",
			"script": "generate-artboard-titles.js"
		},
		{
			"name": "Select Symbol Instances on Page",
			"identifier": "select-symbol-instances",
			"script": "select-symbol-instances.js"
		},
		{
			"name": "Match Instance Name to Symbol Name",
			"identifier": "match-instance-name",
			"script": "match-instance-name.js"
		}
	],
	"menu": {
    	"isRoot": true,
		"items": [
			{
				"title": "SNCR - Layout & Wireframes",
				"items": [
					"artboard-wireframe-layout",
					"create-artboard-bounds",
					"generate-artboard-titles",
				]
			},
			{
				"title": "SNCR - Tools",
				"items": [
					"select-symbol-instances",
					"match-instance-name"
				]
			}
		]
	}
}